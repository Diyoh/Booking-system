APP_NAME="Community Booking System"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=booking_system
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Redis Configuration (Optional - for USSD session caching)
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Africa's Talking Configuration
AT_USERNAME=sandbox
AT_API_KEY=your_africastalking_api_key_here
AT_SENDER_ID=BOOKING
AT_ENVIRONMENT=sandbox

# Ngrok URL for Webhooks (Update when ngrok starts)
NGROK_URL=https://your-ngrok-url.ngrok.io

# Application Settings
BOOKING_HOLD_MINUTES=5
USSD_SESSION_TIMEOUT=180
